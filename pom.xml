			<version>2.3.3</version>
			<scope>provided</scope>
		</dependency>
		
		<!-- https://mvnrepository.com/artifact/org.vivoweb/vivo-languages-webapp-en_US -->
		<dependency>
			<groupId>org.vivoweb</groupId>
			<artifactId>vivo-languages-webapp-en_US</artifactId>
			<version>1.12.2</version>
		</dependency>
		
		

